        "        self.assertEqual(info.name, 'MyClass')",
        "        self.assertEqual(info.docstring_summary, 'A test class.')",
        "        self.assertEqual(len(info.methods), 2)",
        "        self.assertEqual(info.methods[0].name, 'method1')",

        "import os",
        "from typing import Dict",
        "",
        "def func1():",
        "    \"\"\"First function.\"\"\"",
        "    pass",
        "",
        "def func2():",
        "    \"\"\"Second function.\"\"\"",
        "    pass",
        "",
        "class MyClass:",
        "    \"\"\"A class.\"\"\"",
        "    pass",
        "'''",
        "        with open(self.test_file, 'w') as f:",
        "            f.write(code)",

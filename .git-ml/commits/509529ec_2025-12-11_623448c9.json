        "    def test_generates_valid_yaml(self):",
        "        \"\"\"Test that generated output is valid YAML.\"\"\"",
        "        code = '''\"\"\"Test module.\"\"\"",
        "",
        "def test_func():",
        "    \"\"\"A test function.\"\"\"",
        "    pass",
        "'''",
        "        with open(self.test_file, 'w') as f:",
